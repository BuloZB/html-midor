<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width">
	<meta name="description" content="{block metaDescription}{/block}">
	<meta name="author" content="Igor Hlina">
	<meta name="robots" content="{$robots}" n:ifset="$robots">

	<title>{ifset #title}{include #title} :: {/ifset}Midor</title>

	<link rel="shortcut icon" href="{$basePath}/favicon.ico" type="image/x-icon">
	<link rel="stylesheet" href="{$basePath}/css/main.combined.001.css">
	<script src="{$basePath}/js/_libs/modernizr-2.5.3.min.js"></script>
</head>
<body>

	<div class="navbar navbar-fixed-top">
		<div class="navbar-inner">
			<div class="container">
				<a data-target=".nav-collapse" data-toggle="collapse" class="btn btn-navbar">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>

				<a class="brand" href="{$basePath}/">Midor</a>

				<div class="nav-collapse">
					<ul class="nav">
						<li n:class="$presenter->isLinkCurrent('Homepage:default') ? active"><a n:href="Homepage:">home</a></li>
						<li n:class="$presenter->isLinkCurrent('Homepage:nosidebar') ? active"><a n:href="Homepage:nosidebar">nosidebar</a></li>
						<li class="dropdown">
							<a data-toggle="dropdown" class="dropdown-toggle" href="#">dropdown menu <b class="caret"></b></a>
							<ul class="dropdown-menu">
								<li><a href="#">LED</a></li>
								<li><a href="#">OLED</a></li>
								<li><a href="#">Plasma</a></li>
								<li class="divider"></li>
								<li class="nav-header">special types</li>
								<li><a href="#">3D</a></li>
							</ul>
						</li>
					</ul>

					<ul class="nav pull-right">
						<li><a href="#">logout</a></li>
					</ul>

					<form action="" class="navbar-search pull-right">
						<input type="text" placeholder="Search" class="search-query span2">
					</form>
				</div>
			</div>
		</div>
	</div><!-- .navbar -->


	<div class="container">
		<div id="main" role="main">
			{ifset #title}
				<div class="page-header">
					<h1>{include #title} {ifset #supporttext}<small>{include #supporttext}</small>{/ifset}</h1>
				</div>
			{/ifset}

			<div class="row">
				{include #main}
			</div><!-- .row -->
		</div>

		<footer>
			<p>2012 &copy; <a href="https://twitter.com/srigi">srigi</a></p>
		</footer>
	</div> <!-- .container#main -->


	<script src="{$basePath}/js/main.combined.001.js"></script>

</body>
</html>
